<h3>最新上传的设计</h3>
<ul class="thumbnails" >
  <div id="main">
    <% designs.each do |design| %>
      <li class="item">
    <div class="thumbnail">
      <%= link_to :controller => 'designs', :action => 'show',:id => design.id do %>
        <%= image_tag("#{design.image.thumb}",:alt => "#{design.title}", class:"img-rounded") %>
      <p> <%= "#{design.title}" %></p>
    <% end %>
    </div>
    </li>
  <% end %>
  </div>
</ul>
